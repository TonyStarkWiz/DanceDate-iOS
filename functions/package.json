{
  "name": "dancedate-functions",
  "description": "Firebase Functions for DanceDate Apple Pay integration",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.4.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "typescript": "^5.0.0"
  },
  "private": true
}
